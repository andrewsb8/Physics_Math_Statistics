       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "k", " ", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "k", 
