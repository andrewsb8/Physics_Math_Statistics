Cell[TextData[{
 "The inverse square potential is given by V = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox["\[Beta]\[HBar]", "2"], 
    RowBox[{"2", "m"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    SuperscriptBox["r", "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".  Using the Born approximation, f(\[Theta]) = (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "2"}], " ", "m"}], ")"}], "/", 
      RowBox[{"k\[HBar]", "^", "2"}]}], ")"}], 
    UnderscriptBox[
     OverscriptBox["\[Integral]", "\[Infinity]"], "0"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " V Sin[kr] r \[DifferentialD]r. The differential scattering cross section \
is given by \[Sigma](\[Theta]) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{"f", "(", "\[Theta]", ")"}], 
    SuperscriptBox["|", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". "
}], "Text",
 CellChangeTimes->{{3.639335440319242*^9, 3.639335440995688*^9}, {
  3.639335518327242*^9, 3.639335567936569*^9}, {3.639335624883741*^9, 
  3.639335682038184*^9}, {3.6393357266317463`*^9, 3.6393358171403837`*^9}, {
  3.639335895615711*^9, 3.639335933966383*^9}, {3.639335999074127*^9, 
  3.639336001817298*^9}}],

Cell[BoxData[
 RowBox[{"V", ":=", 
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[Beta]", " ", 
      SuperscriptBox["\[HBar]", "2"]}], 
     RowBox[{"2", "m"}]], " ", 
    FractionBox["1", 
     SuperscriptBox["r", "2"]]}],
   TraditionalForm]}]], "Input",
 CellChangeTimes->{{3.6393359522964497`*^9, 3.639335975441667*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"f\[Theta]", " ", "=", " ", 
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{"-", "2"}], " ", "m"}], 
    RowBox[{"k", " ", 
     SuperscriptBox["\[HBar]", "2"]}]], " ", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"V", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"k", " ", "r"}], "]"}], " ", "r"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "Infinity"}], "}"}], ",", " ", 
     RowBox[{"Assumptions", " ", "\[Rule]", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Beta]", ">", "0"}], ",", " ", 
        RowBox[{"m", ">", "0"}], ",", " ", 
        RowBox[{"\[HBar]", ">", "0"}], ",", " ", 
        RowBox[{"k", ">", "0"}]}], "}"}]}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.639335987395958*^9, 3.639336121758583*^9}}],

