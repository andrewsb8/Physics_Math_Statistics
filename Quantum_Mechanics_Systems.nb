Cell[TextData[{
 "This means that the second term in the parentheses goes to \[Infinity].  \
Let me just play with some algebra and expand.\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Sigma]", "t"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"4", "\[Pi]", " ", 
      SuperscriptBox["a", "2"], "En", " ", "m"}], " ", "-", " ", 
     RowBox[{
     "4", " ", "\[Pi]", " ", "a", " ", "En", " ", "m", " ", "\[Infinity]"}], 
     " ", "+", " ", "\[Infinity]"}], 
    RowBox[{
     SuperscriptBox["En", "2"], 
     SuperscriptBox["m", "2"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"4", "\[Pi]", " ", 
     SuperscriptBox["a", 
      RowBox[{"2", " "}]], 
     SuperscriptBox["En", "2"], " ", 
     SuperscriptBox["m", "2"]}], 
    RowBox[{
     SuperscriptBox["En", "2"], 
     SuperscriptBox["m", "2"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " - ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
    "4", "\[Pi]", " ", "a", " ", "En", " ", "m", " ", "\[Infinity]", " "}], 
    RowBox[{
     SuperscriptBox["En", "2"], 
     SuperscriptBox["m", "2"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " + ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Infinity]", 
    RowBox[{
     SuperscriptBox["En", "2"], 
     SuperscriptBox["m", "2"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"4", 
    SuperscriptBox["\[Pi]a", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "- \[Infinity] + \[Infinity]\nSince -\[Infinity]+\[Infinity] is \
indeterminate, "
}], "Text",
